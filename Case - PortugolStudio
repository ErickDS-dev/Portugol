// Uso do caso em PortugolStudio
programa
{
	
	funcao inicio()
	{
		escreva("1- Abrir Netflix 2- Abrir Amazon Prime 3- Abrir HBO GO 4- Sair")
		inteiro menu = 0
		escreva("\n" + "Sua opção")
		leia(menu)
		escolha(menu) {
		caso 1:
		escreva("Abrindo Netflix")
		pare
		caso 2:
		escreva("Abrindo Amazon Prime")
		pare
		caso 3:
		escreva("Abrindo HBO GO")
		pare
		caso 4:
		escreva("Saindo")
		pare
		}
	}
}
